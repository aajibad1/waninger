Resources:
  AWSEBAutoScalingGroup:
    Metadata:
      AWS::CloudFormation::Authentication:
        S3Auth:
          type: "s3"
          buckets: ["elasticbeanstalk-us-east-1-701856502070"]
          roleName:
            "Fn::GetOptionSetting":
              Namespace: "aws:autoscaling:launchconfiguration"
              OptionName: "IamInstanceProfile"
              DefaultValue: "aws-elasticbeanstalk-ec2-role"

container_commands:
    01-mkdir:
        command: "mkdir -p /opt/python/app/current/static/featured"
        ignoreErrors: true
    02-get-data:
        command: "aws s3 cp s3://elasticbeanstalk-us-east-1-701856502070/waninger/static/ . --recursive"
